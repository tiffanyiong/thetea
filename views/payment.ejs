                    <div class="billing-info-wrap">

                        
                        <form action="/thankyou" method="POST">
                       

                    </div>
                    <div class="checkout-account mt-25">
                        <span class="checkout-toggle w-auto h-auto" />
                 
                        <!-- <button class="button" id="hover-item" style="font-size: x-large;"> <i class="pe-7s-map-marker "></i> Pick up THE Tea </button> -->
                        <h2> Payment Methods</h2>
                        <form id="payment-form">
                           
                              
                       <button> <a href="https://buy.stripe.com/test_14k16n1y8fAW3Ru000"> Pay by Stripe Now (TestMode)</a></button>
                          </form>
                    </div>
                  
                    <br><br><br>
            

                    
                    <br> <br> 
                    <form action="/<%=cartId %>/checkout/payment" method="POST">
                    <button style="background-color: #EAAA85; color: white; border-radius: 5px;
                    padding: 15px 30px;" type="submit">
                       Place Order
                    </button>
              

                    <a style =" padding-left: 30px; color: black;"href="/<%=order.order_info.cartId%>/checkout/shipping_method"> Return to shipping</a>
                </form> <!--place order form-->
                    
                </div>
                <div class="col-lg-5 mt-md-30px mt-lm-30px ">
                    <div class="your-order-area">
                        <h3>Your order</h3>
                        <div class="your-order-wrap gray-bg-4">
                            <div class="your-order-product-info">
                                <div class="your-order-top">
                                    <ul>
                                        <li>Product</li>
                                        <li>Total</li>
                                    </ul>
                                </div>
                                <div class="your-order-middle">
                                    <% if (cart === null || cart.items.length == 0) { %>
                                        
                                        <% } else { %>
                                        <ul> 
                                        <% for (let i = 0; i < cart.items.length ; i++) {%>
                                            
                                                <li>
                                                  
                                                   
                                                   <span class="order-middle-left">  
                                                    <img style="width: 50px; height: 50px; margin-right: 5px; " 
                                                    src="<%=cart.items[i].productId.image[0].url%>" /> 
                                                      
                                                     <%= cart.items[i].productId.name %> x <%= cart.items[i].qty%></span>

                                                    <% if (cart.items[i].productId.onsale) { %> 
                                                        <span  style="margin-top:12px;" class="order-price"> $<%= cart.items[i].productId.saleprice %> </span></li>
                                                        <% }  else { %>
                                                            <span  style="margin-top:12px; "class="order-price"> $<%= cart.items[i].productId.price %> </span></li>
                                                            <% }%> 
                                                 
                                          
                                            
                                            <%} %>
                                        </ul>
                                            <%} %>
                                  
                                </div>
                                <div class="your-order-bottom">
                                    <ul>
                                        <% if (cart.promocodes.length > 0 ) { %>
                                            <li class="your-order-shipping">Promocde: <span style="color: #EAAA85;padding-left: 5px;"> 
                                                <i class="fa fa-tag" style="padding-right: 5px;"> </i><%=cart.promocodes[0].promocodeId.code%> </span></li>
                                            <li>
                                            <span style="color:#EAAA85;">
                                                - $ <%=cart.discount%>
                                            </span></li>
                                            <% } %>
                                    </ul>
                                    <ul>
                                        <li class="your-order-shipping">Shipping</li>
                                        <li><span>$<%=order.shipping_info.shipping_fee%></span> </li>
                               
                                    </ul>
                                   
                                </div>
                         
                                <div class="your-order-total">
                                    <ul>
                                        <li class="order-total">Total</li>
                                        <li><span style="color:rgb(171, 167, 167); font-size: small;font-weight: 100; padding-right: 10px;"> HKD </span>
                                            <span style="font-size: x-large ; color: black">  HK$<%=order.order_info.final_total %>  </span></li>
                                    </ul>
                                </div>
                                
                            </div>
                         
                        </div>

                        <form action="/<%=cartId %>/checkout/payment" method="POST">
                    <div class="Place-order mt-25">
                        
                        <a class="btn-hover"><button class="button" type="submit"> Place Order</button></a>
                    </div>
                </form> <!--place order form-->
               
                </form> 
